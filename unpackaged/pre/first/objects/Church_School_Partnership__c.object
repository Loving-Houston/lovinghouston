    <fields>
        <fullName>Quarter_Discovered__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Date_Discovered__c),
&quot;&quot;,
IF(
AND(MONTH(Date_Discovered__c)&gt;0,MONTH(Date_Discovered__c)&lt;4),
&quot;Q1 - &quot;,
IF(
AND(MONTH(Date_Discovered__c)&gt;3,MONTH(Date_Discovered__c)&lt;7),
&quot;Q2 - &quot;,
IF(
AND(MONTH(Date_Discovered__c)&gt;6,MONTH(Date_Discovered__c)&lt;10),
&quot;Q3 - &quot;,
IF(
AND(MONTH(Date_Discovered__c)&gt;9,MONTH(Date_Discovered__c)&lt;13),
&quot;Q4 - &quot;,
&quot;&quot;
))))+TEXT(YEAR(Date_Discovered__c))
)</formula>
